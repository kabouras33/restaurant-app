# .env.example

# General settings
NODE_ENV=production
PORT=3000

# Frontend settings
VITE_APP_TITLE=YourAppName
VITE_API_BASE_URL=https://api.yourapp.com

# Backend settings
API_HOST=0.0.0.0
API_PORT=8000
API_LOG_LEVEL=info

# Database settings
DB_HOST=localhost
DB_PORT=5432
DB_NAME=yourapp_db
DB_USER=yourapp_user
DB_PASSWORD=yourapp_password

# Security settings
SECRET_KEY=your_secret_key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# AWS settings
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=your_aws_region
AWS_S3_BUCKET=your_s3_bucket_name

# Cloudflare settings
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id

# Docker settings
DOCKER_IMAGE_NAME=yourapp_image
DOCKER_CONTAINER_NAME=yourapp_container

# Miscellaneous
LOG_LEVEL=info
MAX_CONNECTIONS=10